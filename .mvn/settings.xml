<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">

  <!-- Server credentials -->
  <servers>
    <!-- You can add server credentials here if needed -->
  </servers>

  <!-- Profiles -->
  <profiles>
    <profile>
      <id>default-profile</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      
      <!-- Repository declarations -->
      <repositories>
        <!-- Maven Central Repository -->
        <repository>
          <id>central</id>
          <name>Maven Central</name>
          <url>https://repo.maven.apache.org/maven2</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        
        <!-- JitPack Repository -->
        <repository>
          <id>jitpack</id>
          <name>JitPack Repository</name>
          <url>https://jitpack.io</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      
      <!-- Plugin Repository declarations -->
      <pluginRepositories>
        <!-- Maven Central Plugin Repository -->
        <pluginRepository>
          <id>central</id>
          <name>Maven Central</name>
          <url>https://repo.maven.apache.org/maven2</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </pluginRepository>
        
        <!-- JitPack Plugin Repository -->
        <pluginRepository>
          <id>jitpack</id>
          <name>JitPack Repository</name>
          <url>https://jitpack.io</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  
  <!-- Active profiles -->
  <activeProfiles>
    <activeProfile>default-profile</activeProfile>
  </activeProfiles>
  
  <!-- Mirrors configuration -->
  <mirrors>
    <!-- Add mirrors if needed -->
  </mirrors>
  
  <!-- Proxies configuration -->
  <proxies>
    <!-- Add proxies if needed -->
  </proxies>
</settings>